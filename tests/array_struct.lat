class list {
  int elem;
  list next;
}



class A {
    int[] x;
}



int main() {
  // tablica struktur
  list[] a = new list[3];
  a[0] = new list;
  a[1] = new list;
  a[2] = new list;
  
  (a[0]).elem = 1;
  (a[0]).next = a[1];
  
  list k = a[1];
  k.elem = 2;
  k.next = a[2];
  
  (a[2]).elem = 3;
  (a[2]).next = (list)null;
  
  printInt((a[0]).elem); // 1
  printInt((a[0]).next.elem); // 2
  list b = a[2];
  printInt(b.elem); // 3
  
  for (list obj : a) // 1 2 3
    printInt(obj.elem);
  
  
  
  // struktura z polem tablicowym
  A[] p = new A[5];
  p[0] = new A;
  (p[0]).x = new int[4];
  (p[0]).x[3] = 57;
  printInt((p[0]).x[3]);

  
  return 0;
}

